ng=false;\r\n    this.orthoTableData=[orthoList];\r\n  },\r\n  showsnpInfoTable(snpId,dataSource,orthoList){\r\n     console.log(\"gwasid,taxid:\",snpId);\r\n     this.$axios.get(\"http://192.168.164.14:9042/gvmRESTV2/v2/variants/getlist?dataSource=\"+dataSource+\"&snplist=\"+snpId)\r\n     .then(response=>{\r\n        console.log(\"gwas response:\",response);\r\n        this.gwasInfoData=response.data.data;\r\n        this.varLoading=false;\r\n     })\r\n    this.showVarTableBox = true;\r\n    this.showOrthoSubTable=true;\r\n    this.orthoTableData=[orthoList];\r\n    this.orthoLoading=false;\r\n    console.log(orthoList);\r\n   },\r\n  },\r\n  \r\n  mounted() {\r\n      this.$axios.get(\"http://localhost:9401/api/variants\",{params:{'classification': this.classification}})\r\n      .then((response) => {\r\n        this.totalSize = response.data.recordsTotal;\r\n        this.loading = false\r\n        console.log(\"response:\",response);\r\n        this.variantData = response.data.data;\r\n        console.log(\"response.data.data.length:\",response.data.data.length);\r\n      if(this.variantData.length>0){\r\n          for(let varData of this.variantData){\r\n            // 增加一个属性保存物种的index，用来显示表格里的icon\r\n            varData.speciesListData=[];\r\n            if(varData.orthoList){\r\n              for(let item of varData.orthoList){\r\n                let speciesName=item.species.commonName\r\n                if(this.speciesList.indexOf(speciesName)==-1){\r\n                  this.speciesList.push(speciesName);\r\n                }\r\n                varData.speciesListData.push(this.speciesList.indexOf(speciesName));\r\n              }\r\n            }\r\n          }\r\n          console.log(\"variantData:\",this.variantData);\r\n        }\r\n\r\n\r\n      })\r\n      window.addEventListener(\"scroll\", this.watchScroll);\r\n      this.getTableMaxHeight(); \r\n      let _this = this;\r\n      window.onresize = function () {//用于使表格高度自适应的方法  \r\n      _this.getTab